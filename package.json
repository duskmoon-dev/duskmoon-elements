    "release": "bun run release:base && bun run release:elements && bun run release:bundle",
    "release:base:dry-run": "bun run --filter @duskmoon-dev/el-base release:dry-run",
    "release:elements:dry-run": "bun run --filter '@duskmoon-dev/el-button' --filter '@duskmoon-dev/el-card' --filter '@duskmoon-dev/el-input' --filter '@duskmoon-dev/el-markdown' --filter '@duskmoon-dev/el-switch' --filter '@duskmoon-dev/el-alert' --filter '@duskmoon-dev/el-dialog' --filter '@duskmoon-dev/el-badge' --filter '@duskmoon-dev/el-chip' --filter '@duskmoon-dev/el-tooltip' --filter '@duskmoon-dev/el-progress' --filter '@duskmoon-dev/el-form' --filter '@duskmoon-dev/el-slider' --filter '@duskmoon-dev/el-file-upload' --filter '@duskmoon-dev/el-autocomplete' --filter '@duskmoon-dev/el-datepicker' --filter '@duskmoon-dev/el-select' --filter '@duskmoon-dev/el-cascader' --filter '@duskmoon-dev/el-bottom-navigation' --filter '@duskmoon-dev/el-breadcrumbs' --filter '@duskmoon-dev/el-drawer' --filter '@duskmoon-dev/el-menu' --filter '@duskmoon-dev/el-navbar' --filter '@duskmoon-dev/el-pagination' --filter '@duskmoon-dev/el-stepper' --filter '@duskmoon-dev/el-tabs' --filter '@duskmoon-dev/el-accordion' --filter '@duskmoon-dev/el-bottom-sheet' --filter '@duskmoon-dev/el-popover' --filter '@duskmoon-dev/el-table' release:dry-run",
    "release:bundle:dry-run": "bun run --filter @duskmoon-dev/elements release:dry-run",
    "release:dry-run": "bun run release:base:dry-run && bun run release:elements:dry-run && bun run release:bundle:dry-run",
    "playground": "bun run --filter @duskmoon-dev/playground dev",
    "playground:build": "bun run --filter @duskmoon-dev/playground build",
    "playground:preview": "bun run --filter @duskmoon-dev/playground preview",
    "docs": "bun run --filter @duskmoon-dev/docs dev",
    "docs:dev": "bun run --filter @duskmoon-dev/docs dev",
    "docs:build": "bun run --filter @duskmoon-dev/docs build",
    "docs:preview": "bun run --filter @duskmoon-dev/docs preview"
  },
  "devDependencies": {
    "@duskmoon-dev/core": "1.10.1",
    "@eslint/js": "9.39.2",
    "@types/bun": "^1.1.14",
    "del-cli": "7.0.0",
    "eslint": "9.39.2",
    "globals": "16.5.0",
    "happy-dom": "20.1.0",
    "prettier": "3.7.4",
    "typescript": "^5.7.2",
    "typescript-eslint": "8.51.0"
  }
}
