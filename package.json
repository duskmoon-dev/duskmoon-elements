{
  "name": "@duskmoon-dev/custom-elements",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "workspaces": [
    "elements/*",
    "packages/*",
    "playground"
  ],
  "scripts": {
    "build": "bun run --filter '*' build",
    "dev": "bun run --filter '*' dev",
    "test": "bun test",
    "typecheck": "bun run --filter '*' typecheck",
    "clean": "bun run --filter '*' clean",
    "build:base": "bun run --filter @duskmoon-dev/el-base build",
    "build:elements": "bun run --filter '@duskmoon-dev/el-button' --filter '@duskmoon-dev/el-card' --filter '@duskmoon-dev/el-input' --filter '@duskmoon-dev/el-markdown' --filter '@duskmoon-dev/el-switch' --filter '@duskmoon-dev/el-alert' --filter '@duskmoon-dev/el-dialog' --filter '@duskmoon-dev/el-badge' --filter '@duskmoon-dev/el-chip' --filter '@duskmoon-dev/el-tooltip' --filter '@duskmoon-dev/el-progress' --filter '@duskmoon-dev/el-form' --filter '@duskmoon-dev/el-slider' --filter '@duskmoon-dev/el-file-upload' --filter '@duskmoon-dev/el-autocomplete' --filter '@duskmoon-dev/el-datepicker' --filter '@duskmoon-dev/el-select' --filter '@duskmoon-dev/el-cascader' --filter '@duskmoon-dev/el-bottom-navigation' --filter '@duskmoon-dev/el-breadcrumbs' --filter '@duskmoon-dev/el-drawer' --filter '@duskmoon-dev/el-menu' --filter '@duskmoon-dev/el-navbar' --filter '@duskmoon-dev/el-pagination' --filter '@duskmoon-dev/el-stepper' --filter '@duskmoon-dev/el-tabs' --filter '@duskmoon-dev/el-accordion' --filter '@duskmoon-dev/el-bottom-sheet' --filter '@duskmoon-dev/el-popover' --filter '@duskmoon-dev/el-table' build",
    "build:bundle": "bun run --filter @duskmoon-dev/elements build",
    "build:all": "bun run build:base && bun run build:elements && bun run build:bundle",
    "format": "bun run --filter '*' format",
    "format:check": "bun run --filter '*' format:check",
    "lint": "bun run --filter '*' lint",
    "lint:check": "bun run --filter '*' lint:check",
    "lint:fix": "bun run --filter '*' lint:fix",
    "release:base": "bun run --filter @duskmoon-dev/el-base release",
    "release:elements": "bun run --filter '@duskmoon-dev/el-button' --filter '@duskmoon-dev/el-card' --filter '@duskmoon-dev/el-input' --filter '@duskmoon-dev/el-markdown' --filter '@duskmoon-dev/el-switch' --filter '@duskmoon-dev/el-alert' --filter '@duskmoon-dev/el-dialog' --filter '@duskmoon-dev/el-badge' --filter '@duskmoon-dev/el-chip' --filter '@duskmoon-dev/el-tooltip' --filter '@duskmoon-dev/el-progress' --filter '@duskmoon-dev/el-form' --filter '@duskmoon-dev/el-slider' --filter '@duskmoon-dev/el-file-upload' --filter '@duskmoon-dev/el-autocomplete' --filter '@duskmoon-dev/el-datepicker' --filter '@duskmoon-dev/el-select' --filter '@duskmoon-dev/el-cascader' --filter '@duskmoon-dev/el-bottom-navigation' --filter '@duskmoon-dev/el-breadcrumbs' --filter '@duskmoon-dev/el-drawer' --filter '@duskmoon-dev/el-menu' --filter '@duskmoon-dev/el-navbar' --filter '@duskmoon-dev/el-pagination' --filter '@duskmoon-dev/el-stepper' --filter '@duskmoon-dev/el-tabs' --filter '@duskmoon-dev/el-accordion' --filter '@duskmoon-dev/el-bottom-sheet' --filter '@duskmoon-dev/el-popover' --filter '@duskmoon-dev/el-table' release",
