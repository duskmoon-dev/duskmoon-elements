{
  "name": "@duskmoon-dev/elements",
  "version": "0.6.0",
  "description": "All DuskMoon custom elements in one package",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./register": {
      "types": "./dist/types/register.d.ts",
      "import": "./dist/esm/register.js",
      "require": "./dist/cjs/register.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "bun run clean",
    "build": "bun run build:esm && bun run build:cjs && bun run build:types",
    "build:esm": "bun build ./src/index.ts ./src/register.ts --outdir ./dist/esm --format esm --sourcemap --external @duskmoon-dev/el-base --external @duskmoon-dev/el-button --external @duskmoon-dev/el-card --external @duskmoon-dev/el-input --external @duskmoon-dev/el-markdown --external @duskmoon-dev/el-switch --external @duskmoon-dev/el-alert --external @duskmoon-dev/el-dialog --external @duskmoon-dev/el-badge --external @duskmoon-dev/el-chip --external @duskmoon-dev/el-tooltip --external @duskmoon-dev/el-progress --external @duskmoon-dev/el-form --external @duskmoon-dev/el-slider --external @duskmoon-dev/el-file-upload --external @duskmoon-dev/el-autocomplete --external @duskmoon-dev/el-datepicker --external @duskmoon-dev/el-bottom-navigation --external @duskmoon-dev/el-breadcrumbs --external @duskmoon-dev/el-drawer --external @duskmoon-dev/el-menu --external @duskmoon-dev/el-navbar --external @duskmoon-dev/el-pagination --external @duskmoon-dev/el-stepper --external @duskmoon-dev/el-tabs --external @duskmoon-dev/el-accordion --external @duskmoon-dev/el-bottom-sheet --external @duskmoon-dev/el-popover --external @duskmoon-dev/el-table",
    "build:cjs": "bun build ./src/index.ts ./src/register.ts --outdir ./dist/cjs --format cjs --sourcemap --external @duskmoon-dev/el-base --external @duskmoon-dev/el-button --external @duskmoon-dev/el-card --external @duskmoon-dev/el-input --external @duskmoon-dev/el-markdown --external @duskmoon-dev/el-switch --external @duskmoon-dev/el-alert --external @duskmoon-dev/el-dialog --external @duskmoon-dev/el-badge --external @duskmoon-dev/el-chip --external @duskmoon-dev/el-tooltip --external @duskmoon-dev/el-progress --external @duskmoon-dev/el-form --external @duskmoon-dev/el-slider --external @duskmoon-dev/el-file-upload --external @duskmoon-dev/el-autocomplete --external @duskmoon-dev/el-datepicker --external @duskmoon-dev/el-bottom-navigation --external @duskmoon-dev/el-breadcrumbs --external @duskmoon-dev/el-drawer --external @duskmoon-dev/el-menu --external @duskmoon-dev/el-navbar --external @duskmoon-dev/el-pagination --external @duskmoon-dev/el-stepper --external @duskmoon-dev/el-tabs --external @duskmoon-dev/el-accordion --external @duskmoon-dev/el-bottom-sheet --external @duskmoon-dev/el-popover --external @duskmoon-dev/el-table",
    "build:types": "tsc --emitDeclarationOnly --outDir ./dist/types",
    "clean": "del-cli dist",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write 'src/**/*.ts' '*.json' '*.md'",
    "format:check": "prettier --check 'src/**/*.ts' '*.json' '*.md'",
    "lint": "eslint src",
    "lint:check": "eslint src --max-warnings 0",
    "lint:fix": "eslint src --fix",
    "release": "bun publish",
    "release:dry-run": "bun publish --dry-run"
  },
  "dependencies": {
    "@duskmoon-dev/el-base": "workspace:*",
    "@duskmoon-dev/el-button": "workspace:*",
    "@duskmoon-dev/el-card": "workspace:*",
    "@duskmoon-dev/el-input": "workspace:*",
    "@duskmoon-dev/el-markdown": "workspace:*",
    "@duskmoon-dev/el-switch": "workspace:*",
    "@duskmoon-dev/el-alert": "workspace:*",
    "@duskmoon-dev/el-dialog": "workspace:*",
    "@duskmoon-dev/el-badge": "workspace:*",
    "@duskmoon-dev/el-chip": "workspace:*",
    "@duskmoon-dev/el-tooltip": "workspace:*",
    "@duskmoon-dev/el-progress": "workspace:*",
    "@duskmoon-dev/el-form": "workspace:*",
    "@duskmoon-dev/el-slider": "workspace:*",
    "@duskmoon-dev/el-file-upload": "workspace:*",
    "@duskmoon-dev/el-autocomplete": "workspace:*",
    "@duskmoon-dev/el-datepicker": "workspace:*",
    "@duskmoon-dev/el-bottom-navigation": "workspace:*",
    "@duskmoon-dev/el-breadcrumbs": "workspace:*",
    "@duskmoon-dev/el-drawer": "workspace:*",
    "@duskmoon-dev/el-menu": "workspace:*",
    "@duskmoon-dev/el-navbar": "workspace:*",
    "@duskmoon-dev/el-pagination": "workspace:*",
    "@duskmoon-dev/el-stepper": "workspace:*",
    "@duskmoon-dev/el-tabs": "workspace:*",
    "@duskmoon-dev/el-accordion": "workspace:*",
    "@duskmoon-dev/el-bottom-sheet": "workspace:*",
    "@duskmoon-dev/el-popover": "workspace:*",
    "@duskmoon-dev/el-table": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "mermaid": ">=10.0.0"
  },
  "peerDependenciesMeta": {
    "mermaid": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "duskmoon-dev",
    "duskmoon",
    "custom-elements",
    "customelements",
    "web-components",
    "webcomponents",
    "html"
  ]
}
